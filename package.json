{
  "publisher": "OSpoon",
  "name": "dep-cmd",
  "displayName": "DepCmd",
  "version": "0.0.11",
  "private": true,
  "packageManager": "pnpm@10.4.1",
  "description": "一个专为开发者设计的 VS Code 扩展，旨在简化开发阶段各种终端命令的使用，减少记忆负担，提高开发效率。",
  "author": "OSpoon <zxin088@gmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/OSpoon",
  "homepage": "https://github.com/OSpoon/DepCmd#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/OSpoon/DepCmd"
  },
  "bugs": {
    "url": "https://github.com/OSpoon/DepCmd/issues"
  },
  "sponsor": {
    "url": "https://github.com/sponsors/OSpoon"
  },
  "categories": [
    "Other"
  ],
  "main": "./dist/index.js",
  "icon": "res/icon.png",
  "files": [
    "LICENSE.md",
    "dist/*",
    "res/*"
  ],
  "engines": {
    "vscode": "^1.74.0"
  },
  "galleryBanner": {
    "color": "#1e1e1e",
    "theme": "dark"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "depCmd",
          "title": "Dependencies Commands",
          "icon": "res/icon.png"
        }
      ]
    },
    "views": {
      "depCmd": [
        {
          "id": "depCmdView",
          "name": "Commands",
          "icon": "res/icon.png"
        }
      ]
    },
    "configuration": {
      "title": "DepCmd",
      "properties": {
        "depCmd.enableProjectDetection": {
          "type": "boolean",
          "default": true,
          "description": "Enable automatic project detection to filter commands based on project type"
        },
        "depCmd.defaultCategory": {
          "type": "string",
          "default": "all",
          "description": "Default category to display (all or specific category name)"
        },
        "depCmd.projectDetection": {
          "type": "object",
          "default": {
            "nodejs": ["package.json"],
            "npm": ["package-lock.json"],
            "yarn": ["yarn.lock"],
            "pnpm": ["pnpm-lock.yaml"],
            "python": ["requirements.txt", "pyproject.toml", "setup.py", "Pipfile"],
            "rust": ["Cargo.toml"],
            "go": ["go.mod"]
          },
          "description": "项目检测配置：每个键是项目类型，值是要检测的文件/目录模式数组。检测器会根据这些模式判断项目类型，然后自动提取相应的运行命令。开发者可以自由修改检测模式，但命令提取逻辑由检测器内部实现。",
          "additionalProperties": {
            "type": "array",
            "items": {
              "type": "string"
            }
          }
        },
        "depCmd.dependencyDetection": {
          "type": "object",
          "default": {
            "git": {
              "command": "git --version",
              "enabled": true,
              "description": "检测 Git 是否安装"
            },
            "docker": {
              "command": "docker --version",
              "enabled": true,
              "description": "检测 Docker 是否安装"
            },
            "nrm": {
              "command": "nrm --version",
              "enabled": true,
              "description": "检测 NRM (npm registry manager) 是否安装"
            },
            "conda": {
              "command": "conda --version",
              "enabled": true,
              "description": "检测 Conda 是否安装"
            },
            "python": {
              "command": "python --version",
              "enabled": true,
              "description": "检测 Python 是否安装"
            },
            "npm": {
              "command": "npm --version",
              "enabled": true,
              "description": "检测 NPM 是否安装"
            }
          },
          "description": "依赖检测配置：定义用于检测系统工具是否安装的命令。只有检测成功的工具对应的分组才会显示。",
          "additionalProperties": {
            "type": "object",
            "properties": {
              "command": {
                "type": "string",
                "description": "用于检测依赖是否安装的命令"
              },
              "enabled": {
                "type": "boolean",
                "description": "是否启用此依赖检测"
              },
              "description": {
                "type": "string",
                "description": "依赖检测的描述"
              }
            }
          }
        },
        "depCmd.categoryDisplay": {
          "type": "object",
          "default": {
            "npm": {
              "displayName": "NPM",
              "icon": "package"
            },
            "yarn": {
              "displayName": "Yarn",
              "icon": "package"
            },
            "pnpm": {
              "displayName": "PNPM",
              "icon": "package"
            },
            "git": {
              "displayName": "Git",
              "icon": "git-branch"
            },
            "docker": {
              "displayName": "Docker",
              "icon": "server-process"
            },
            "python": {
              "displayName": "Python",
              "icon": "symbol-class"
            },
            "rust": {
              "displayName": "Cargo",
              "icon": "gear"
            },
            "go": {
              "displayName": "Go",
              "icon": "symbol-function"
            },
            "nrm": {
              "displayName": "NRM",
              "icon": "settings"
            },
            "conda": {
              "displayName": "Conda",
              "icon": "symbol-namespace"
            }
          },
          "description": "分类显示配置：自定义每个分类的显示名称和图标。用户可以添加新分类或修改现有分类的显示信息。",
          "additionalProperties": {
            "type": "object",
            "properties": {
              "displayName": {
                "type": "string",
                "description": "分类的显示名称"
              },
              "icon": {
                "type": "string",
                "description": "分类的图标名称（VS Code 图标）"
              }
            },
            "required": ["displayName", "icon"]
          }
        }
      }
    },
    "commands": [
      {
        "command": "depCmd.sendToTerminal",
        "title": "Send to Terminal",
        "icon": "$(terminal)"
      },
      {
        "command": "depCmd.refreshView",
        "title": "Refresh",
        "icon": "$(refresh)"
      },
      {
        "command": "depCmd.searchCommands",
        "title": "Search Commands",
        "icon": "$(search)"
      },
      {
        "command": "depCmd.copyCommand",
        "title": "Copy Command",
        "icon": "$(copy)"
      },
      {
        "command": "depCmd.editCommand",
        "title": "Edit Command",
        "icon": "$(edit)"
      },
      {
        "command": "depCmd.deleteCommand",
        "title": "Delete Command",
        "icon": "$(trash)"
      },
      {
        "command": "depCmd.addCommand",
        "title": "Add New Command",
        "icon": "$(add)"
      },
      {
        "command": "depCmd.editCategory",
        "title": "Rename Category",
        "icon": "$(edit)",
        "category": "Dependencies Commands"
      },
      {
        "command": "depCmd.deleteCategory",
        "title": "Delete Category",
        "icon": "$(trash)",
        "category": "Dependencies Commands"
      },
      {
        "command": "depCmd.toggleProjectDetection",
        "title": "Toggle Project Detection",
        "icon": "$(eye)",
        "category": "Dependencies Commands"
      },
      {
        "command": "depCmd.clearAllData",
        "title": "Clear All Command Data",
        "icon": "$(trash)",
        "category": "Dependencies Commands"
      },
      {
        "command": "depCmd.exportData",
        "title": "Export Command Data",
        "icon": "$(export)",
        "category": "Dependencies Commands"
      },
      {
        "command": "depCmd.importData",
        "title": "Import Command Data",
        "icon": "$(import)",
        "category": "Dependencies Commands"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "depCmd.addCommand",
          "when": "view == depCmdView",
          "group": "navigation@1"
        },
        {
          "command": "depCmd.searchCommands",
          "when": "view == depCmdView",
          "group": "navigation@2"
        },
        {
          "command": "depCmd.refreshView",
          "when": "view == depCmdView",
          "group": "navigation@3"
        },
        {
          "command": "depCmd.importData",
          "when": "view == depCmdView",
          "group": "management@1"
        },
        {
          "command": "depCmd.exportData",
          "when": "view == depCmdView",
          "group": "management@2"
        },
        {
          "command": "depCmd.clearAllData",
          "when": "view == depCmdView",
          "group": "management@3"
        }
      ],
      "view/item/context": [
        {
          "command": "depCmd.sendToTerminal",
          "when": "view == depCmdView && viewItem == command",
          "group": "inline@1"
        },
        {
          "command": "depCmd.editCommand",
          "when": "view == depCmdView && viewItem == command",
          "group": "inline@2"
        },
        {
          "command": "depCmd.copyCommand",
          "when": "view == depCmdView && viewItem == command",
          "group": "inline@3"
        },
        {
          "command": "depCmd.deleteCommand",
          "when": "view == depCmdView && viewItem == command",
          "group": "inline@4"
        },
        {
          "command": "depCmd.sendToTerminal",
          "when": "view == depCmdView && viewItem == project-script",
          "group": "inline@1"
        },
        {
          "command": "depCmd.copyCommand",
          "when": "view == depCmdView && viewItem == project-script",
          "group": "inline@2"
        },
        {
          "command": "depCmd.editCategory",
          "when": "view == depCmdView && viewItem == category",
          "group": "inline@1"
        },
        {
          "command": "depCmd.deleteCategory",
          "when": "view == depCmdView && viewItem == category",
          "group": "inline@2"
        }
      ]
    }
  },
  "scripts": {
    "build": "tsdown src/index.ts --external vscode",
    "dev": "nr build --watch --sourcemap",
    "prepare": "nr update",
    "update": "vscode-ext-gen --output src/generated/meta.ts",
    "lint": "eslint .",
    "vscode:prepublish": "nr build",
    "publish": "vsce publish --no-dependencies",
    "pack": "vsce package --no-dependencies",
    "test": "vitest",
    "test:run": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "release": "bumpp"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.19.0",
    "@antfu/ni": "^23.3.1",
    "@types/node": "^22.17.2",
    "@types/vscode": "^1.74.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vscode/vsce": "^3.6.0",
    "bumpp": "^10.2.3",
    "eslint": "^9.33.0",
    "esno": "^4.8.0",
    "pnpm": "^10.15.0",
    "reactive-vscode": "^0.2.22",
    "tsdown": "^0.9.9",
    "typescript": "^5.9.2",
    "vite": "^6.3.5",
    "vitest": "^3.2.4",
    "vscode-ext-gen": "^1.1.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}
